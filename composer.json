{
  "name": "jessekoska/swagger-lume",
  "description": "Swagger integration to Lumen 5",
  "keywords": ["lumen", "laravel", "swagger"],
  "license": "MIT",
  "authors": [
    {
      "name": "Jesse Koska",
      "email": "jessekoska@gmail.com"
    },
    {
      "name": "Darius Matulionis",
      "email": "darius@matulionis.lt"
    },
    {
      "name": "Martin Abelson Sahlen",
      "email": "sahlen@stud.ntnu.no"
    }
  ],
  "require": {    
    "php": ">=5.5.9",
    "laravel/lumen-framework": "~5.0",
    "zircote/swagger-php": "~2.0"
  },
  "require-dev": {
    "vlucas/phpdotenv": "^2.2",
    "codeclimate/php-test-reporter": "^0.2.0"
  },
  "autoload": {
    "psr-4": {
      "SwaggerLume\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/LumenTestCase.php"
    ],
    "files": [
      "tests/ConsoleKernel.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
}
